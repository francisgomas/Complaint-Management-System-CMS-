<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>

    <!-- Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700">
    <!-- Icons -->
    <link rel="stylesheet" href="~/panel/nucleo/css/nucleo.css" type="text/css">
    <link rel="stylesheet" href="~/panel/fortawesome/fontawesome-free/css/all.min.css" type="text/css">
    <link rel="stylesheet" href="~/panel/datatables.net-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/panel/datatables.net-buttons-bs4/css/buttons.bootstrap4.min.css">
    <link rel="stylesheet" href="~/panel/datatables.net-select-bs4/css/select.bootstrap4.min.css">
    <!-- Argon CSS -->
    <link rel="stylesheet" href="~/panel/argon.css?v=1.1.0" type="text/css">
</head>

<body>
   
	@RenderBody()

    <script src="~/panel/jquery/dist/jquery.min.js"></script>
    <script src="~/panel/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/panel/js-cookie/js.cookie.js"></script>
    <script src="~/panel/jquery.scrollbar/jquery.scrollbar.min.js"></script>
    <script src="~/panel/jquery-scroll-lock/dist/jquery-scrollLock.min.js"></script>

    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}

    <!-- Argon JS -->
    <script src="~/panel/argon.js?v=1.1.0"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
